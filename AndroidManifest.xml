<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uci.asleepawake"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="10" />
    <uses-permission android:name="android.permission.INTERNET"/>
<uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS"/>
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.uci.asleepawake.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.uci.asleepawake.DisplayMessageActivity"
            android:label="@string/title_activity_display_message" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.Form"
            android:label="@string/title_activity_form" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.Sleepiness"
            android:label="@string/title_activity_sleepiness" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.SleepinessGeneral"
            android:label="@string/title_activity_sleepiness_general" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.Relationship"
            android:label="@string/title_activity_relationship" >
        </activity>
        <activity
            android:name="edu.uci.asleepawake.FeelRightNow"
            android:label="@string/title_activity_feel_right_now" >
        </activity>   
    </application>

</manifest>